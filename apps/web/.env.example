# Configuration de l'API
NEXT_PUBLIC_API_URL=http://localhost:3001/api
NEXT_PUBLIC_API_VERSION=v1

# Configuration d'authentification
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=votre_secret_nextauth_ultra_securise
JWT_SECRET=votre_secret_jwt_ultra_securise

# Configuration multi-tenant
NEXT_PUBLIC_DEFAULT_TENANT=default
NEXT_PUBLIC_ENABLE_SUBDOMAIN_ROUTING=true

# Configuration des fonctionnalités
NEXT_PUBLIC_ENABLE_COMMAND_PALETTE=true

# Configuration du système de design
NEXT_PUBLIC_ENABLE_ANIMATIONS=true
NEXT_PUBLIC_DEFAULT_THEME=dark

# Configuration de l'environnement
NODE_ENV=development

# Configuration des intégrations
NEXT_PUBLIC_MAPBOX_TOKEN=votre_token_mapbox

# Configuration de l'application
NEXT_PUBLIC_APP_NAME=PLANNER Suite
NEXT_PUBLIC_APP_DESCRIPTION=Suite modulaire de planification pour différents secteurs d'activité
